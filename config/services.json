{
    "file_loader.json": {
        "class": "FileLoader.JsonLoader"
    },
    "config.loader.parameters": {
        "class": "Config.ParametersLoader",
        "arguments": [
            "@file_loader.json"
        ]
    },
    "http_client": {
        "class": "Http.HttpClient"
    },
    "router": {
        "class": "Router.Router",
        "arguments": [
            "@file_loader.json"
        ]
    },
    "data_fetcher.index": {
        "class": "DataFetcher.IndexFetcher",
        "arguments": [
            "@http_client",
            "@router"
        ]
    },
    "data_fetcher.terms": {
        "class": "DataFetcher.TermsFetcher",
        "arguments": [
            "@http_client",
            "@router"
        ]
    }
}